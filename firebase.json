{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public",
    "rewrites": [
      { "source": "/processNLP", "function": "processNLP"}
    ],
    "headers": [
      {
        "source": "/",
        "headers": [{"key": "Link", "value": "</js/demo.js>;rel=preload;as=script,</css/style.css>;rel=preload;as=style"}]
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
